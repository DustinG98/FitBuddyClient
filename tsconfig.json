{
    "compilerOptions": {
      "allowJs": true,
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "isolatedModules": true,
      "jsx": "react-native",
      "lib": ["es6"],
      "moduleResolution": "node",
      "noEmit": true,
      "strict": true,
      "skipLibCheck": true,
      "target": "es2017"
    },
    "exclude": ["node_modules"],
    "extends": "expo/tsconfig.base"
  }